{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "sayingwedding",
          "appId": "1:676660103213:android:9ff96f92d0d3ead6b0d0a4",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "sayingwedding",
          "appId": "1:676660103213:ios:813c8cd5fd92d25ab0d0a4",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "macos": {
        "default": {
          "projectId": "sayingwedding",
          "appId": "1:676660103213:ios:813c8cd5fd92d25ab0d0a4",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "sayingwedding",
          "configurations": {
            "android": "1:676660103213:android:9ff96f92d0d3ead6b0d0a4",
            "ios": "1:676660103213:ios:813c8cd5fd92d25ab0d0a4",
            "macos": "1:676660103213:ios:813c8cd5fd92d25ab0d0a4",
            "web": "1:676660103213:web:7bc6bdf4be507440b0d0a4",
            "windows": "1:676660103213:web:e68966c70f9508f1b0d0a4"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" },
          { "key": "Pragma", "value": "no-cache" },
          { "key": "Expires", "value": "0" }
        ]
      },
      {
        "source": "/assets/audio/**",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}